{
    "name": "finagin/laravel-ldap",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Igor Finagin",
            "email": "Igor@Finag.in",
            "homepage": "https://Finag.in",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-ldap": "*",
        "illuminate/support": "~5.8.0|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Finagin\\LaravelLdap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Finagin\\LaravelLdap\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Finagin\\LaravelLdap\\LdapServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
